{
  "version": 2,
  "name": "dijinew-api",
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CLIENT_URL": "https://dijinew.vercel.app",
    "BACKEND_API_URL": "https://dijinew-api.vercel.app"
  },
  "build": {
    "env": {
      "CLIENT_URL": "https://dijinew.vercel.app",
      "BACKEND_API_URL": "https://dijinew-api.vercel.app"
    }
  }
}

